[global]
thread
direct=1
ioengine=libaio
filename=/dev/rbd0
per_job_logs=0
group_reporting
iodepth=8
bs=128K

[128K-write-new]
numjobs=4
size=12800M
rw=write
write_bw_log
write_lat_log

[128K-write-overlap]
numjobs=4
size=12800M
rw=write
write_bw_log
write_lat_log


[128K-read]
stonewall
numjobs=4
rw=read
write_iops_log
write_lat_log
